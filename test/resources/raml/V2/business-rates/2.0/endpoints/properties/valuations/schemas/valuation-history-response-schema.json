                "required": [
                  "href",
                  "template"
                ],
                "properties": {
                  "href": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "^https?://\\w+",
                    "description": "URL for this link"
                  },
                  "template": {
                    "type": "boolean",
                    "description": "Is the HREF a RFC-6570 compliant URI Template"
                  }
                }
              },
              "detailedValuation": {
                "type": "object",
                "description": "Link to the detailed valuation.",
                "required": [
                  "href"
                ],
                "properties": {
                  "href": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "^https?://\\w+",
                    "description": "URL for this link"
                  }
                }
              }
            }
          },
          "address": {
            "type": "string",
            "description": "Address"
          },
          "billingAuthorityCode": {
            "type": "string",
            "description": "Local authority code"
          },
          "billingAuthorityReference": {
            "type": "string",
            "description": "Local authority reference"
          },
          "compositeProperty": {
            "type": "string",
            "description": "Part domestic (composite) property"
          },
          "costPerM2": {
            "type": "number",
            "format": "number",
            "description": "Base rate perm2/unit"
          },
          "currentFromDate": {
            "type": "string",
            "format": "date",
            "description": "Date this valuation was/is current FROM"
          },
          "currentToDate": {
            "type": "string",
            "format": "date",
            "description": "Date this valuation was current TO"
          },
