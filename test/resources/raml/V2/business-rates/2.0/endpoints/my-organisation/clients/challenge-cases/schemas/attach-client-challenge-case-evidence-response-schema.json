{
  "title": "PreparedUpload",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "uploadRequest",
    "reference"
  ],
  "properties": {
    "reference": {
      "title": "Reference",
      "type": "string"
    },
    "uploadRequest": {
      "title": "UploadFormTemplate",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "fields",
        "href"
      ],
      "properties": {
        "fields": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "x-amz-meta-callback-url": {
              "type": "string"
            },
            "x-amz-algorithm": {
              "type": "string"
            },
            "x-amz-signature": {
              "type": "string"
            },
            "key": {
              "type": "string"
            },
            "acl": {
              "type": "string"
            },
            "x-amz-date": {
              "type": "string"
            },
            "x-amz-credential": {
              "type": "string"
            },
            "Content-Type": {
              "type": "string"
            },
            "policy": {
              "type": "string"
            },
            "success_action_redirect": {
              "type": "string",
              "format": "uri"
            },
            "error_action_redirect": {
              "type": "string",
              "format": "uri"
            }
          },
          "items": {
            "type": "string"
          }
        },
        "href": {
          "type": "string",
          "format": "uri",
          "pattern": "^https?://\\w+"
        }
      }
    }
  }
}
