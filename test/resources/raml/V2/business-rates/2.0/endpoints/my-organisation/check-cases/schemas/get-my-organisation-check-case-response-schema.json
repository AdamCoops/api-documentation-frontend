{
  "title": "HateoasCheckCasesWithAgents",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "_links",
    "submittedBy",
    "createdDateTime",
    "uarn",
    "address",
    "checkCaseStatus",
    "checkCaseReference",
    "checkCaseSubmissionId"
  ],
  "properties": {
    "_links": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "self": {
          "type": "object",
          "required": [
            "template",
            "href"
          ],
          "properties": {
            "href": {
              "type": "string",
              "format": "uri",
              "pattern": "^https?://\\w+",
              "description": "URL for this link"
            },
            "template": {
              "type": "boolean",
              "description": "Is the HREF a RFC-6570 compliant URI Template"
            }
          }
        }
      }
    },
    "address": {
      "type": "string",
      "description": "The address of the property"
    },
    "agent": {
      "type": "object",
      "description": "Details of the current agent assigned to the property.",
      "required": [
        "organisationName",
        "code",
        "organisationId"
      ],
      "properties": {
        "code": {
          "type": "number",
          "format": "number",
          "description": "The unique code of the agent"
        },
        "organisationId": {
          "type": "number",
          "format": "number",
          "description": "Unique ID for the agent organisation"
        },
        "organisationName": {
          "type": "string",
          "description": "The name of the agent organisation"
        }
      },
      "additionalProperties": false
    },
    "checkCaseReference": {
      "type": "string",
      "description": "The reference number for this check case"
    },
    "checkCaseStatus": {
      "type": "string",
      "enum": [
        "RECEIVED",
        "PENDING",
        "ASSIGNED",
        "OPEN",
        "DECISION_SENT",
        "UNDER_REVIEW",
        "CLOSED",
        "CANCELLED"
      ]
    },
    "checkCaseSubmissionId": {
      "type": "string",
      "description": "The submission ID for this check case"
    },
    "createdDateTime": {
      "type": "string",
      "description": "The date/time this check case was created"
    },
    "settledDate": {
      "type": "string",
      "description": "The date this check case was settled"
    },
    "submittedBy": {
      "type": "string",
      "description": "The organisation that submitted check case"
    },
    "uarn": {
      "type": "number",
      "format": "number",
      "description": "The unique address reference of the property"
    }
  }
}